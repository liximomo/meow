{
  "name": "meow",
  "productName": "Meow",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^2.12.3",
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.28.4",
    "dot-prop-immutable": "^1.3.1",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "^4.0.0",
    "html-webpack-plugin": "2.29.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "20.0.4",
    "lodash-es": "^4.17.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "purify-css": "^1.2.5",
    "react": "^15.6.1",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.10.5",
    "reselect": "^3.0.1",
    "shell-env": "^0.3.0",
    "split.js": "^1.3.5",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "uuid": "^3.1.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-manifest-plugin": "1.1.0"
  },
  "main": "main.js",
  "scripts": {
    "start": "ELECTRON_START_URL=http://localhost:3000 node scripts/start.js",
    "build": "PUBLIC_URL=./ node scripts/build.js",
    "build:mac": "electron-packager ./ Meow --platform=darwin --arch=x64 --overwrite --out ./dist/$npm_package_version",
    "prebuild:mac": "rm -rf ./dist",
    "test": "node scripts/test.js --env=jsdom",
    "app": "electron ."
  },
  "devDependencies": {
    "babel-plugin-import": "^1.3.1",
    "electron": "^1.7.5",
    "electron-packager": "^8.7.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        [
          {
            "libraryName": "antd",
            "style": true
          }
        ]
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
